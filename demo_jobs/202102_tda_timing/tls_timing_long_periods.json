{
  "job_name": "tls_timing_long_periods",
  "task_list": [
    {
      "task": "execution_for_loop",
      "name": "cadence",
      "values": [
        25,
        50,
        600
      ],
      "task_list": [
        {
          "task": "execution_for_loop",
          "name": "lc_length",
          "values": [
            "3 * constants.month",
            "4 * constants.month",
            "6 * constants.month",
            "9 * constants.month",
            "constants.year",
            "1.5 * constants.year",
            "2 * constants.year"
          ],
          "task_list": [
            {
              "task": "psls_synthesise",
              "outputs": {
                "lightcurve": "ttlp_${cadence_index}_${lc_length_index}.gz"
              },
              "specs": {
                "duration": "${lc_length}",
                "enable_transits": 0,
                "sampling_cadence": "${cadence}"
              }
            },
            {
              "task": "transit_search",
              "inputs": {
                "lightcurve": "ttlp_${cadence_index}_${lc_length_index}.gz"
              },
              "lc_duration": "${lc_length}",
              "tda_name": "tls",
              "search_settings": {
                "period_min": 90
              }
            }
          ]
        }
      ]
    }
  ]
}
