{
  "job_name": "earth_vs_snr",
  "task_list": [
    {
      "task": "execution_for_loop",
      "name": "nsr",
      "log_range": [
        "73/5",
        "73*5",
        "10"
      ],
      "task_list": [
        {
          "task": "psls_synthesise",
          "outputs": {
            "lightcurve": "evs_${nsr_index}.gz"
          },
          "specs": {
            "duration": 730,
            "planet_radius": "constants.Rearth",
            "orbital_period": 240,
            "semi_major_axis": 1,
            "orbital_angle": 0,
            "nsr": "${nsr}"
          }
        },
        {
          "task": "verify",
          "inputs": {
            "lightcurve": "evs_${nsr_index}.gz"
          }
        },
        {
          "task": "transit_search",
          "inputs": {
            "lightcurve": "evs_${nsr_index}.gz"
          },
          "lc_duration": 730,
          "tda_name": "tls"
        }
      ]
    }
  ]
}
